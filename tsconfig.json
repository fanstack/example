{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
			"@fan-interfaces": [
				"./src/app/fan-interfaces"
			],
			"@fan-services": [
				"./src/app/fan-services"
			],
			"@fan-services/functions": [
				"./src/app/fan-services/functions"
			],
		}
	}
}